{
  "presets": [
    "@babel/preset-env"
  ],
  "env": {
    "development": {
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-syntax-class-properties",
        "@babel/plugin-proposal-object-rest-spread"
      ]
    },
    "production": {
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-syntax-class-properties",
        "@babel/plugin-proposal-object-rest-spread"
      ]
    }
  }
}
